{
    "name": "zomars/instagram",
    "description": "Get raw feed data from Instagram's public API",
    "type": "bolt-extension",
    "keywords": [
        "instagram",
        "social",
        "feed"
    ],
    "require": {
        "php": ">=7.0",
        "bolt/bolt": "^3.0",
        "vinkla/instagram": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "zomars",
            "email": "zomars@me.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Zomars\\Instagram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Zomars\\Instagram\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Zomars\\Instagram\\InstagramExtension"
    }
}
